<h1>Listing Products</h1>
<%= link_to 'New product', new_product_path %>

<table>
  <tr>
    <th>Title</th>
    <th>Price</th>
    <th>Available Inventory</th>
    <th colspan="3"></th>
  </tr>

  <% @products.each do |product| %>
    <tr>
      <td><%= product.title %></td>
      <td><%= product.price %></td>
      <td><%= product.available_inventory %></td>
    </tr>
  <% end %>
</table>
